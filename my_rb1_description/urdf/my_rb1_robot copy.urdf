<robot name = my_rb1_robot>

    <link name="base_link">
        <inertial>
            <mass value="25" />
            <origin xyz="0 0 0.3" rpy="0 0 0" />
            <inertia ixx="0.578" ixy="0" ixz="0" iyy="0.578" iyz="0" izz="0.78" />
        </inertial>

        <collision>
            <geometry>
                <cylinder length="0.3" radius = "0.25"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder length="0.3" radius = "0.25"/>
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1" />
            </material>
        </visual>
    </link>


    <link name="right_wheel">
        <inertial>
            <mass value="25" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
        </inertial>

        <collision>
            <geometry>
                <cylinder length="0.03" radius = "0.025"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder length="0.03" radius = "0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>


    <link name="left_wheel">
        <inertial>
            <mass value="25" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
        </inertial>

        <collision>
            <geometry>
                <cylinder length="0.03" radius = "0.025"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder length="0.03" radius = "0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <link name="back_caster">
        <inertial>
            <mass value="25" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
        </inertial>

        <collision>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>

    <link name="front_caster">
        <inertial>
            <mass value="25" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
        </inertial>

        <collision>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>

    <link name="front_laser">
        <origin xyz="0 0 0" rpy="0 0 0" />
    </link>

    <link name="base_footprint">
            
    </link>

    <joint name="base_footprint" type="fixed">
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <origin xyz="0 -0.3 0" rpy="0 0 0" />
    </joint>

    <joint name="laser_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_laser"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>

    <joint name="rwheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0, -0.2, -0.15" rpy="0 0 0" />
        
    </joint>

    <joint name="lwheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0, 0.2, -0.15" rpy="0 0 0" />
    </joint>

    <joint name="fcaster_joint" type="continuous">
        <parent link="base_link"/>
        <child link="front_caster"/>
        <origin xyz="0.1, 0, -0.15" rpy="0 0 0" />
        <joint_properties  friction="0.0" />
    </joint>

    <joint name="bcaster_joint" type="continuous">
        <parent link="base_link"/>
        <child link="back_caster"/>
        <origin xyz="-0.1, 0, -0.15" rpy="0 0 0" />
        <joint_properties  friction="0.0" />
    </joint>
    

</robot>